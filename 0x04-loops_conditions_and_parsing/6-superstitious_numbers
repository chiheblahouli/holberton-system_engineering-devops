#!/usr/bin/env bash
#displays numbers 1 to 20 with different numbers printing different messages

n=1
while [ $n -le 20 ]
do
    echo "$n"
    case $n in
	4)
	    echo "bad luck from China"
	    ;;
	9)
	    echo "bad luck from Japan"
	    ;;
	17)
	    echo "bad luck from Italy"
	    ;;
    esac
    ((n++))
don
